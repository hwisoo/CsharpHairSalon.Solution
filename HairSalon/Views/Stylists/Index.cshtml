@{
  Layout = "_Layout";
}

<h2 class="text-center">All Stylists</h2>
    @if (@Model.Count == 0)
    {
    <h3>No Stylists are listed!</h3>
    }

    @foreach (var stylist in Model)
    {
    <h3><a href='/stylists/@stylist.GetId()'>@stylist.GetName()</a></h3>
    
    <h3><a href="/stylists/@stylist.GetId()/delete">Delete Stylist</a></h3>
    <br>
    }